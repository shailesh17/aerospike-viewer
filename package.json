{
  "name": "aerospike-viewer",
  "version": "1.0.0",
  "description": "Fullstack Aerospike viewer with monorepo layout",
  "main": "server/server.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --project tsconfig.json --outDir dist/server server/server.ts",
    "start": "node dist/server/server.js",
    "dev:server": "ts-node-dev --respawn --transpile-only server/server.ts",
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "build:server": "tsc --project tsconfig.json --outDir dist/server server/server.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.29.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@vitejs/plugin-react": "^5.1.0",
    "aerospike": "^6.4.0",
    "autoprefixer": "^10.4.21",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "vite": "^7.2.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
